{
  "name": "docs",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:docs": "node dist/docs/server/server.mjs",
    "lint": "ng lint",
    "format": "prettier --write ."
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-organize-imports"
    ],
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.8.0",
  "dependencies": {
    "@angular/cdk": "^21.1.1",
    "@angular/common": "^21.1.2",
    "@angular/compiler": "^21.1.2",
    "@angular/core": "^21.1.2",
    "@angular/forms": "^21.1.2",
    "@angular/material": "^21.1.1",
    "@angular/platform-browser": "^21.1.2",
    "@angular/platform-server": "^21.1.2",
    "@angular/router": "^21.1.2",
    "@angular/ssr": "^21.1.2",
    "@fontsource/roboto-condensed": "^5.2.8",
    "@material-symbols/font-500": "^0.40.2",
    "express": "^5.2.1",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular/build": "^21.1.2",
    "@angular/cli": "^21.1.2",
    "@angular/compiler-cli": "^21.1.2",
    "@types/express": "^5.0.6",
    "@types/node": "^20.17.19",
    "angular-eslint": "21.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "jsdom": "^27.4.0",
    "ng-packagr": "^21.1.0",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "8.54.0",
    "vitest": "^4.0.18"
  }
}
